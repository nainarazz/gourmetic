{
	"version": 2,
	"builds": [
		{ "src": "src/package.json", "use": "@now/next" },
		{ "src": "api/index.ts", "use": "@now/node-server" }
	],
	"routes": [
		{ "src": "/api", "dest": "/api/index.ts" },
		{ "src": "^/recipe/(?<name>[^/]+)", "dest": "/src/recipe?name=$name" },
		{ "src": "/(.*)", "dest": "src/$1" }
	]
}
